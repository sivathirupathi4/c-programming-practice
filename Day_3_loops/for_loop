#include <stdio.h>
int main(){
    int n;
    printf("Enter the n value: ");
    scanf("%d\n",&n);
    int a;
    printf("Enter the first val: ");
    scanf("%d",&a);
    int max = a;
    for (int i=1;i<=n;i++){
        scanf("%d",&a);
        if (a>max){
            max=a;
            printf("max=a\n");
            

        }
        printf("Max: %d\n",max);
    }
}
